{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            true,
            160
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "trailing-comma": [
            true,
            "never"
        ],
        "member-access": [
            true,
            "no-public"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": [
            true,
            {
                "grouped-imports": true,
                "import-sources-order": "any",
                "named-imports-order": "any"
            }
        ],
        "no-console": false,
        "prefer-for-of": false,
        "jsx-boolean-value": false,
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/generated/*"
        ]
    }
}