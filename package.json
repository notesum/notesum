    "devDependencies": {
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-static-files-copy": "^2.6.0",
        "tslint": "^6.1.2",
        "tslint-react": "^5.0.0"
    },
    "scripts": {
        "clean": "rimraf dist",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "build": "yarn run clean && yarn run lint && yarn run build:parcel",
        "build:parcel": "parcel build public/index.html --public-url / --no-source-maps",
        "dev": "parcel public/index.html",
        "preview": "git rev-parse --abbrev-ref HEAD | xargs -I {} bash -c 'firebase hosting:channel:deploy {}'",
        "live": "git rev-parse --abbrev-ref HEAD | xargs -I {} bash -c 'firebase hosting:clone notesum-front:{} notesum-front:live'"
    },
    "staticFiles": {
        "staticPath": [
            {
                "staticPath": "public/.well-known",
                "staticOutDir": ".well-known"
            }
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}